{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "test": "NODE_ENV=test  yarn run db:migrate && NODE_ENV=test  nyc --reporter=text --reporter=lcov mocha --require babel-polyfill --require @babel/register tests/*  --timeout 10000 --exit",
    "start": "NODE_ENV=development nodemon --exec babel-node index.js",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:reset": "NODE_ENV=test node_modules/.bin/sequelize db:migrate:undo:all",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "webpack-dev-server --open",
    "transpile": "webpack"
  },
  "keywords": [
    "sass",
    "react"
  ],
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80
  },
  "author": " Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/register": "^7.0.0",
    "@sendgrid/mail": "^6.3.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^3.2.0",
    "bcrypt": "^3.0.3",
    "cors": "^2.8.4",
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "method-override": "^2.3.10",
    "moment": "^2.24.0",
    "morgan": "^1.9.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.9",
    "opn": "^5.4.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "request": "^2.87.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "sinon": "^7.2.4",
    "slug": "^0.9.3",
    "swagger-ui-express": "^4.0.2",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0",
    "prettier": "^1.16.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "production": {
    "use_env_variable": "DATABASE_URL"
  }
}
